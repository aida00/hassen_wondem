{#
/**
 * @file
 * Default theme implementation for checkout panes.
 *
 * Available variables:
 * - elements: The checkout pane's form elements.
 *
 * @ingroup themeable
 */
#}
<div class="review-wrapper lg:grid lg:grid-cols-4 lg:gap-6 lg:pb-3">
  <h2 class=" text-black font-semibold text-2xl lg:text-3xl mb-1 lg:col-span-4">Renter/Patient Details</h2>
  <div class="review-contact mb-5 pb-5 border-b border-gray-300 border-dashed lg:border-0 lg:col-span-4 lg:mb-2 lg:pb-0">
    {{ elements.contact_information }}
  </div>
  <div class="review-shipping mb-5 pb-5 border-b border-gray-300 border-dashed lg:border-0 lg:col-span-2 lg:mb-0 lg:pb-0">
    {{ elements.shipping_information }}
  </div>
  <div class="review-payment lg:col-span-2">
    {{ elements.payment_information }}
  </div>
</div>
