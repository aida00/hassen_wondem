{
    "name": "pantheon-upstreams/drupal-composer-managed",
    "description": "Install Drupal 9 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "path",
            "url": "upstream-configuration"
        }
    ],
    "require": {
        "pantheon-upstreams/upstream-configuration": "dev-main",
        "composer/installers": "^1.9",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-recommended": "^10",
        "pantheon-systems/drupal-integrations": "^9",
        "cweagans/composer-patches": "^1.7",
        "drupal/asset_injector": "^2.12",
        "drupal/domain": "^2.0@beta",
        "drupal/extlink": "^2.0",
        "drupal/field_group": "^3.3",
        "drupal/jquery_ui_accordion": "^2.1",
        "drupal/simple_gmap": "^3.0",
        "drupal/token_filter": "^2.2",
        "drupal/address": "^1.11",
        "drupal/admin_toolbar": "^3.1",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/block_visibility_groups": "^2.0",
        "drupal/color_field": "^3.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/commerce": "^2.31",
        "drupal/commerce_currency_resolver": "^1.60",
        "drupal/commerce_exchanger": "^1.21",
        "drupal/commerce_shipping": "^2.4",
        "drupal/commerce_stock": "^1.0",
        "drupal/contact_block": "^1.6",
        "drupal/ctools": "^4.0",
        "drupal/entity": "^1.4",
        "drupal/entity_reference_facet_link": "^2.0",
        "drupal/entity_reference_revisions": "^1.10",
        "drupal/facets": "^2.0",
        "drupal/facets_pretty_paths": "^1.2",
        "drupal/field_timer": "^2.0",
        "drupal/flag": "^4.0@beta",
        "drupal/image_delta_formatter": "^1.1",
        "drupal/jquery_ui_datepicker": "^1.3",
        "drupal/jquery_ui_touch_punch": "^1.1",
        "drupal/lang_dropdown": "^2.0",
        "drupal/redirect": "^1.8",
        "drupal/search_api": "^1.37",
        "drupal/simplenews": "^3.0@beta",
        "drupal/video_embed_field": "^2.5",
        "drupal/votingapi": "^3.0@beta",
        "drupal/votingapi_widgets": "^1.0@alpha",
        "drupal/bootstrap": "^5.0",
        "drupal/google_analytics": "^4.0",
        "drupal/robotstxt": "^1.4",
        "drupal/search404": "^2.1",
        "drupal/unpublished_404": "^2.0",
        "drupal/scheduler": "^2.0@RC",
        "drupal/imageapi_optimize": "^4.1@beta",
        "drupal/amp": "^3.5",
        "drupal/blazy": "^3.0",
        "drupal/xmlsitemap": "^1.4",
        "drupal/file_delete_ui": "^1.0@beta",
        "drupal/bootstrap5": "^3.0",
        "drupal/gutenberg": "^2.7",
        "drupal/paragraphs": "^1.18",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/imagemagick": "^4.0",
        "drupal/db_maintenance": "^3.0@RC",
        "drupal/taxonomy_unique": "^2.5",
        "drupal/image_effects": "^4.0@RC",
        "drupal/content_access": "^2.0",
        "drupal/jquery_ui": "^1.7",
        "drupal/views_simple_sitemap_export": "^1.0@beta",
        "drupal/yoast_seo": "^1.9",
        "drupal/simple_sitemap_auth": "^2.1",
        "drupal/simple_sitemap": "^4.2",
        "drupal/faqfield": "^7.1",
        "drupal/core-project-message": "^10",
        "drush/drush": "^12",
        "drupal/file_mdm": "^3.1",
        "drupal/sophron": "^2.1",
        "drupal/stable": "^2.0",
        "drupal/webprofiler": "^10.3"
    },
    "require-dev": {
        "drupal/core-dev": "^10"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "allowed-packages": ["pantheon-systems/drupal-integrations"],
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "web/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "enable-patching": true
    },
    "autoload": {
        "classmap": ["upstream-configuration/scripts/ComposerScripts.php"]
    },
    "scripts": {
        "pre-update-cmd": ["DrupalComposerManaged\\ComposerScripts::preUpdate"],
        "post-update-cmd": ["DrupalComposerManaged\\ComposerScripts::postUpdate"]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": false,
        "platform": {
            "php": "8.1.13"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true,
            "drupal/core-project-message": true,
            "tbachert/spi": true
        }
    }
}
